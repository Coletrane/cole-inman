{
  "name": "cole-inman",
  "version": "1.0.0",
  "description": "Personal web site / resume for Cole Inman",
  "scripts": {
    "dev": "next",
    "dev8080": "next -p 8080",
    "clean": "rimraf .next out/",
    "webshots": "node ./webshots.js",
    "build": "next build && next export",
    "static": "node ./static.js",
    "lint": "eslint --fix **/*.js *.js --ignore-pattern node_modules --ignore-pattern out --ignore-pattern .next",
